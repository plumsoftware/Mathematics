<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        ads:layout_constraintBottom_toBottomOf="parent"
        ads:layout_constraintEnd_toEndOf="parent"
        ads:layout_constraintStart_toStartOf="parent"
        ads:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginHorizontal="8dp"
                android:layout_weight="0"
                android:elevation="0dp"
                android:fontFamily="@font/roboto"
                android:gravity="center"
                android:text="0"
                android:textColor="?attr/colorGreen"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/notRight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginHorizontal="8dp"
                android:layout_weight="0"
                android:elevation="0dp"
                android:fontFamily="@font/roboto"
                android:gravity="center"
                android:text="0"
                android:textColor="?attr/colorRed"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Space
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/rAnswer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="8dp"
                android:layout_weight="0"
                android:elevation="0dp"
                android:fontFamily="@font/roboto"
                android:gravity="center"
                android:padding="8dp"
                android:textColor="#000000"
                android:textSize="20sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="8dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginBottom="16dp"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/l1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="0"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textViewBigTask"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0"
                        android:fontFamily="@font/roboto"
                        android:foregroundGravity="center"
                        android:gravity="center|start"
                        android:padding="4dp"
                        android:textColor="?attr/textColor"
                        android:textSize="16sp"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/bigAnswer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="0"
                        android:elevation="0dp"
                        android:fontFamily="@font/roboto"
                        android:foregroundGravity="center"
                        android:gravity="center|start"
                        android:maxLength="10"
                        android:padding="4dp"
                        android:text="   "
                        android:textColor="?attr/textColor"
                        android:textSize="24sp"
                        android:visibility="visible" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/l2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="0"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/task"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:elevation="0dp"
                        android:fontFamily="@font/roboto_medium"
                        android:foregroundGravity="center"
                        android:gravity="center|end"
                        android:maxLength="70"
                        android:padding="4dp"
                        android:textColor="?attr/textColor"
                        android:textSize="20sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/answer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:elevation="0dp"
                        android:fontFamily="@font/roboto"
                        android:gravity="center|start"
                        android:maxLength="10"
                        android:padding="4dp"
                        android:text="   "
                        android:textColor="?attr/textColor"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/notation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="0"
                        android:elevation="0dp"
                        android:fontFamily="@font/roboto_medium"
                        android:gravity="center|start"
                        android:padding="4dp"
                        android:textColor="?attr/textColor"
                        android:textSize="20sp" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:background="#E4E4E4"
                android:orientation="vertical">

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_weight="0"
                    android:shrinkColumns="6">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="?attr/colorSurface"
                        android:layout_weight="0"
                        android:weightSum="0">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardA"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="0dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPadding="8dp">

                            <TextView
                                android:id="@+id/less"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:text="@string/less"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardB"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="0dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPadding="8dp">

                            <TextView
                                android:id="@+id/more"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:text="@string/more"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardC"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="0dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPadding="8dp">

                            <TextView
                                android:id="@+id/equal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:text="@string/equal"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardD"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="0dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPadding="8dp">

                            <TextView
                                android:id="@+id/minus"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:text="@string/minus"
                                android:textColor="#636363"
                                android:textSize="34sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardE"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="0dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPadding="8dp">

                            <TextView
                                android:id="@+id/fraction"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:text="@string/fraction"
                                android:textColor="#636363"
                                android:textSize="34sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardF"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="0dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPadding="8dp">

                            <TextView
                                android:id="@+id/dot"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:text="@string/dot"
                                android:textColor="#636363"
                                android:textSize="34sp" />
                        </androidx.cardview.widget.CardView>

                    </TableRow>

                </TableLayout>

                <TableLayout
                    android:id="@+id/tableLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:layout_weight="0"
                    android:background="#E4E4E4"
                    android:gravity="center">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="8dp"
                        android:layout_weight="0"
                        android:weightSum="0">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardOne"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="1"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardTwo"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView4"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="2"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardThree"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView9"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="3"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="8dp"
                        android:layout_weight="0"
                        android:weightSum="0">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardFour"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView10"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="4"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardFive"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView11"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="5"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardSix"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="6"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="8dp"
                        android:layout_weight="0"
                        android:weightSum="0">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardSeven"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView13"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="7"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardEight"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView14"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="8"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardNine"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView15"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="9"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="8dp"
                        android:layout_weight="0"
                        android:weightSum="0">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardBackspace"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardBackgroundColor="#00FFFFFF"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <ImageView
                                android:id="@+id/imageView7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:foregroundGravity="center"
                                android:onClick="onNumberClick"
                                android:paddingLeft="8dp"
                                android:paddingTop="8dp"
                                android:paddingRight="8dp"
                                android:paddingBottom="8dp"
                                ads:srcCompat="@drawable/ic_backspace" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardZero"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:onClick="onNumberClick"
                            ads:cardCornerRadius="4dp"
                            ads:cardElevation="0dp"
                            ads:cardUseCompatPadding="false"
                            ads:contentPaddingBottom="8dp"
                            ads:contentPaddingLeft="24dp"
                            ads:contentPaddingRight="24dp"
                            ads:contentPaddingTop="8dp">

                            <TextView
                                android:id="@+id/textView17"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:background="@drawable/btn_menu_click"
                                android:fontFamily="@font/roboto"
                                android:gravity="center"
                                android:onClick="onNumberClick"
                                android:text="0"
                                android:textColor="#636363"
                                android:textSize="30sp" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardDone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
